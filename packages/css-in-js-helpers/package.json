{
  "name": "css-in-js-helpers",
  "version": "1.0.3",
  "private": true,
  "description": "CSS in JS helpers",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "update-css": "node scripts/css/completions.js",
    "update-rn": "node scripts/rn/completions.js",
    "build": "esbuild ./src/index --format=cjs --bundle --platform=node --outfile=./dist/index.js",
    "format": "prettier \"src/**/*\" --write -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XantreGodlike/css-in-js.git"
  },
  "keywords": [
    "CSSinJS"
  ],
  "author": "Ansuman Shah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XantreGodlike/css-in-js/issues"
  },
  "homepage": "https://github.com/XantreGodlike/css-in-js#readme",
  "devDependencies": {
    "esbuild": "^0.17.11"
  },
  "dependencies": {
    "react-docgen": "^2.18.0",
    "request": "^2.82.0"
  }
}
