{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "format": {},
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "compile-tests": {
      "outputs": ["dist-test/**"],
      "inputs": ["scripts/**", "test/**"]
    },
    "test": {
      "dependsOn": ["compile-tests", "^build"]
    },
    "vscode:publish": {
      "dependsOn": ["^build"]
    },
    "lint": {}
  }
}
